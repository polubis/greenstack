{
  "name": "forms",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "forms/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist\\{projectRoot}",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/forms",
        "main": "forms/src/index.ts",
        "tsConfig": "forms/tsconfig.lib.json",
        "assets": ["forms/*.md"]
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist\\{projectRoot}"
      }
    }
  }
}
